{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "not-rely-on-time": "off",
    "no-empty-blocks": "off",
    "func-visibility": "off",
    "no-inline-assembly": "off",
